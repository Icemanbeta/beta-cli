  "private": false,
  "gypfile": true,
  "dependencies": {
    "@types/es6-shim": "^0.31.36",
    "@types/glob": "^5.0.35",
    "@types/lodash": "^4.14.106",
    "@types/node": "^9.6.0",
    "@types/shelljs": "^0.7.8",
    "chalk": "^2.3.2",
    "clear": "^0.1.0",
    "commander": "^2.15.0",
    "enquirer": "^1.0.3",
    "figlet": "^1.2.0",
    "franc": "^3.1.1",
    "fuzzy": "^0.1.3",
    "glob": "^7.1.2",
    "iconv": "^2.3.0",
    "langs": "^2.0.0",
    "lodash": "^4.17.5",
    "prompt-autocompletion": "^0.1.2",
    "prompt-input": "^3.0.0",
    "shelljs": "^0.8.1",
    "ts-node": "^6.0.0",
    "typescript": "^2.8.3",
    "uchardet": "^1.6.6",
    "write-pkg": "^3.1.0"
  },
  "devDependencies": {
    "ava": "^1.0.0-beta.3",
    "ava-ts": "^0.24.3"
  },
  "scripts": {
    "test": "ava-ts",
    "test:bundle": "ava-ts --files='test/bundle*'",
    "test:encode": "ava-ts --files='test/encode*'",
    "test:helpers": "ava-ts --files='test/helpers*'",
    "start": "./index.js"
  },
  "bin": {
    "beta": "index.js"
  },
  "ava": {
    "files": [
      "test/**/*.ts"
    ],
    "sources": [
      "test/**/*.ts"
    ],
    "concurrency": 5,
    "failFast": false,
    "failWithoutAssertions": false,
    "compileEnhancements": false
  }
}
