{
  "compilerOptions": {
    "target": "commonjs",
    "lib": [
      "es2017",
      "es2015",
      "es5",
      "es6",
      "dom",
      "scripthost"
    ],
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "module": "es6",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "sourceMap": false,
    "strict": true
  },
  "include": [
    "typings/**/*",
  ]
}
